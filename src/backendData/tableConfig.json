{
  "description": "Calculation Methodology 1, Table 1: Characterization of tested wells - To be reported in Table AA.1.iii of the Reporting Form",
  "columnDefinitions": [
    {
      "headerName": "A",
      "field": "a",
      "editable": true,
      "options": ["Yes", "No"]
    },
    {
      "headerName": "B",
      "field": "b",
      "editable": true,
      "cellStyleRules": [
        {
          "conditions": [{ "field": "a", "value": "No" }],
          "style": { "backgroundColor": "black" }
        }
      ]
    },
    {
      "headerName": "C",
      "field": "c",
      "editable": true
    },
    {
      "headerName": "D",
      "field": "d",
      "editable": false
    },
    {
      "headerName": "E",
      "field": "e",
      "editable": false,
      "calculations": {
        "excel": "=IF(OR($A=\"No\",$B=\"\"),\"\",IF($A=\"No\",IF($B<1,0,1),IF($C<0.5,0,1)))",
        "if": {
          "check": "any",
          "conditions": [
            { "field": "a", "equals": "No" },
            { "field": "a", "equals": "" }
          ],
          "then": "empty",
          "else": {
            "if": {
              "check": "any",
              "conditions": [{ "field": "b", "equals": "No" }],
              "then": "empty",
              "else": 6
            }
          }
        }
      }
    },
    {
      "headerName": "F",
      "field": "f",
      "editable": false
    }
  ]
}
