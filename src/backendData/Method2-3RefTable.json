{
  "tableId": "Method2-3RefTable",
  "description": "Calculation Methodologies 2 & 3, Reference Table",
  "columnDefinitions": [
    {
      "headerName": "unique id",
      "field": "AI",
      "editable": false,
      "excelFormula": "=IF($Method2-3Table1.C$=\"\",\"\",$Method2-3Table1.C$&\"_\"&$Method2-3Table1.E$)"
    },
    {
      "headerName": "no dupes", // i could just use start row, if we have it
      "field": "AJ",
      "editable": false,
      "excelFormula": "=IF(COUNTIF($AI$0$:$AI$0$,$AI$0$)=1,$AI$0$,\"\")"
    },
    {
      "headerName": "no blanks",
      "field": "AK",
      "editable": false,
      "excelFormula": "=IFERROR(INDEX($AJ$1699:$AJ$6698,SMALL((IF(LEN($AJ$1699:$AJ$6698),ROW(INDIRECT(\"1:\"&ROWS($AJ$1699:$AJ$6698))))),ROW(A1)),1),\"\")"
    }
  ]
}
